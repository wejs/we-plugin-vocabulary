{
  "name": "we-plugin-vocabulary",
  "description": "We.js vocabulary plugin",
  "version": "1.1.0",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "devDependencies": {
    "express-mysql-session": "^1.1.1",
    "fs-extra": "^0.30.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mysql": "^2.11.1",
    "rimraf": "^2.5.3",
    "supertest": "^1.2.0",
    "we-core": "^1.8.6",
    "we-plugin-acl": "^1.2.6",
    "we-plugin-auth": "^1.1.3",
    "we-plugin-file": "^1.3.5",
    "we-plugin-page": "^1.0.0",
    "we-plugin-user": "^1.1.0",
    "we-test-tools": "0.3.18",
    "we-theme-site-wejs": "^0.3.31"
  },
  "repository": "wejs/we-plugin-vocabulary",
  "license": "MIT",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "keywords": [
    "wejs-plugin"
  ],
  "wejs": {
    "plugins": {
      "we-plugin-acl": true,
      "we-plugin-auth": true,
      "we-plugin-file": true,
      "we-plugin-page": true,
      "we-plugin-user": true
    },
    "dependencies": {
      "we-plugin-user": true
    }
  }
}
