    "fs-extra": "^8.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "we-core": "^3.1.0",
    "we-plugin-acl": "^1.2.16",
    "we-plugin-auth": "^2",
    "we-plugin-page": "^2.0.2",
    "we-plugin-user": "^2",
    "we-test-tools": "1.0.0",
    "we-theme-site-wejs": "^0.3.31"
  },
  "repository": "wejs/we-plugin-vocabulary",
  "license": "MIT",
