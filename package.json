{
  "name": "we-plugin-vocabulary",
  "description": "We.js vocabulary, terms and tags plugin",
  "version": "2.0.7",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha -b test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "devDependencies": {
    "express-mysql-session": "^2.1.0",
    "fs-extra": "^8.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mysql2": "^1.6.5",
    "supertest": "^4.0.2",
    "we-core": "^2",
    "we-plugin-acl": "^1.2.14",
    "we-plugin-auth": "^2",
    "we-plugin-page": "^2.0.2",
    "we-plugin-user": "^2",
    "we-test-tools": "0.3.22",
    "we-theme-site-wejs": "^0.3.31"
  },
  "repository": "wejs/we-plugin-vocabulary",
  "license": "MIT",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "keywords": [
    "wejs-plugin"
  ],
  "wejs": {
    "plugins": {
      "we-plugin-acl": true,
      "we-plugin-auth": true,
      "we-plugin-page": true,
      "we-plugin-user": true
    },
    "dependencies": {
      "we-plugin-user": true
    }
  }
}
